#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules/uglify-js/bin/uglifyjs/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules/uglify-js/bin/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules/uglify-js/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules:/home/rivolli/NetBeansProjects/Vaadin/node_modules:/home/rivolli/NetBeansProjects/node_modules:/home/rivolli/node_modules:/home/node_modules:/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/uglify-js/bin/uglifyjs/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/uglify-js/bin/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/uglify-js/node_modules" "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
  ret=$?
else 
  NODE_PATH="/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules/uglify-js/bin/uglifyjs/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules/uglify-js/bin/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules/uglify-js/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/3.4.10/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/uglify-js/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/.pnpm/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules:/home/rivolli/NetBeansProjects/Vaadin/node_modules:/home/rivolli/NetBeansProjects/node_modules:/home/rivolli/node_modules:/home/node_modules:/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/uglify-js/bin/uglifyjs/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/uglify-js/bin/node_modules:/home/rivolli/NetBeansProjects/Vaadin/webapp/node_modules/uglify-js/node_modules" node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
  ret=$?
fi
exit $ret
